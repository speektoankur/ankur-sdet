        <listener class-name="testUtil.TestLogger"></listener>
    </listeners>
    <test name="Smoke UI Test" thread-count="2">
        <classes>
            <class name="UserInterfaceChromeTest"/>
        </classes>
    </test>
    <test name="Smoke Backend Test" thread-count="1">
        <classes>
            <class name="BackEndFunctionalTest"/>
        </classes>
    </test>
</suite>
